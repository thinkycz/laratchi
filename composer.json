{
  "name": "tomchochola/laratchi",
  "description": "Laratchi",
  "license": "proprietary",
  "type": "library",
  "keywords": [
    "tomchochola",
    "laratchi",
    "laravel"
  ],
  "authors": [
    {
      "name": "Tomáš Chochola",
      "email": "chocholatom1997@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.20",
    "redeyeventures/geopattern": "^1.1",
    "spatie/once": "^3.1"
  },
  "require-dev": {
    "composer/composer": "^2.5",
    "fakerphp/faker": "^1.23",
    "phpunit/phpunit": "^10.3"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Tomchochola\\Laratchi\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tomchochola\\Laratchi\\Support\\ServiceProvider"
      ]
    }
  }
}
