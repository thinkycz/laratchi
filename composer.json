{
  "name": "tomchochola/laratchi",
  "description": "Laratchi",
  "license": "proprietary",
  "type": "library",
  "keywords": [
    "tomchochola",
    "laratchi"
  ],
  "authors": [
    {
      "name": "Tomáš Chochola",
      "email": "chocholatom1997@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "intervention/validation": "^3.2",
    "laravel/framework": "^9.10",
    "spatie/laravel-validation-rules": "^3.2",
    "spatie/once": "^3.1"
  },
  "require-dev": {
    "composer/composer": "^2.0",
    "orchestra/testbench": "^7.4",
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Tomchochola\\Laratchi\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tomchochola\\Laratchi\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tomchochola\\Laratchi\\Support\\ServiceProvider"
      ]
    }
  }
}
