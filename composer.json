{
  "name": "tomchochola/laratchi",
  "description": "laratchi",
  "type": "library",
  "keywords": [
    "tomchochola",
    "laratchi"
  ],
  "homepage": "https://github.com/tomchochola/laratchi",
  "readme": "README.md",
  "license": "proprietary",
  "authors": [
    {
      "name": "Tomáš Chochola",
      "email": "chocholatom1997@gmail.com",
      "homepage": "https://github.com/tomchochola",
      "role": "Author"
    }
  ],
  "support": {
    "email": "chocholatom1997@gmail.com",
    "issues": "https://github.com/tomchochola/laratchi/issues",
    "source": "https://github.com/tomchochola/laratchi.git",
    "docs": "https://github.com/tomchochola/laratchi"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/tomchochola"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.20",
    "redeyeventures/geopattern": "^1.1",
    "spatie/once": "^3.1"
  },
  "require-dev": {
    "fakerphp/faker": "@stable",
    "phpunit/phpunit": "@stable",
    "tomchochola/php-cs-fixer": "@dev",
    "tomchochola/phpstan-rules": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Tomchochola\\Laratchi\\": "src"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tomchochola/php-cs-fixer.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/tomchochola/phpstan-rules.git"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "preferred-install": "dist",
    "audit": {
      "abandoned": "fail"
    },
    "optimize-autoloader": true,
    "platform-check": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tomchochola\\Laratchi\\Support\\ServiceProvider"
      ]
    }
  }
}
